<public:component lightweight="true">
<public:attach event="onmouseover" onevent="On()"/>
<public:attach event="onmouseout" onevent="Off()"/>
<script language="JavaScript">
	function On()
	{
	
		if (disabled)
		{
			return;
		}
		src = src.replace(/_off_/,"_on_");
	}
	function Off()
	{
		if (disabled)
		{
			return;
		}
		src = src.replace(/_on_/,"_off_");
	}
</script>
</public:component>
