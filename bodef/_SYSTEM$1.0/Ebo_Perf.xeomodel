    <events>
        <event name="onAfterSave">
            <body language="JAVA">
            	netgest.bo.security.securityRights.cleanPerfomerPolicies();
netgest.bo.utils.AliasUtils.setAlias(this);
            </body>
        </event>
        <event name="onBeforeSave">
        	<body language='JAVA'>
        		return netgest.bo.models.UserOperations.processPass( this ) &amp;&amp;
	(super.onBeforeSave( event ) &amp;&amp; netgest.bo.events.MailAccountEvents.inactiveMailAccounts(this,"emailAccounts"));
        	</body>
        </event>
        <event name="onBeforeDestroy">
            <body language="JAVA">try
{
	netgest.bo.runtime.boObjectList list = netgest.bo.runtime.boObjectList.list(this.getEboContext(), "select Ebo_Alias where ui = " + this.getBoui());
	ArrayList r = new ArrayList();
	while(list.next())
	{
		list.getObject().destroy();
	}
	return true;
}catch(Exception e)
{}
return false;</body>
        </event>
    </events>
