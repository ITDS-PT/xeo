					<valid/>
					<renderAsLov/>
					<hiddenWhen/>
					<required/>
					<actionType/>
				</variable>
				<variable name="Bool">
					<type>boolean</type>
					<label>Booleano</label>
					<objectfilter>
						<filter>
							<BOL_condition/>
							<BOQL_query/>
						</filter>
					</objectfilter>
					<VAR_ADDRESS><![CDATA[]]></VAR_ADDRESS>
					<minoccurs>0</minoccurs>
					<maxoccurs>1</maxoccurs>
					<formula/>
					<lov/>
					<defaultValue>false</defaultValue>
					<valid/>
					<renderAsLov/>
					<hiddenWhen/>
					<required/>
					<actionType/>
				</variable>
				<variable name="Obj" input="true">
					<type>object.Aluno</type>
					<label>Objecto Aluno</label>
					<objectfilter>
						<filter>
							<BOL_condition/>
							<BOQL_query/>
						</filter>
					</objectfilter>
					<VAR_ADDRESS><![CDATA[]]></VAR_ADDRESS>
					<minoccurs>0</minoccurs>
					<maxoccurs>N</maxoccurs>
					<formula/>
					<lov/>
					<defaultValue/>
					<valid/>
					<renderAsLov/>
					<hiddenWhen/>
					<required/>
					<actionType/>
				</variable>
			</variables>
			<participants>
				<participant name="User">
					<type>Object</type>
					<label>Utilizador</label>
					<VAR_ADDRESS/>
					<defaultValue/>
				</participant>
			</participants>
			<xepCode>
				<condition/>
				<codetorun>Texto = "Super"; Bool = true;</codetorun>
			</xepCode>
			<label name="beforeTH"/>
			<fillVariable assync="true">
				<participant/>
				<condition/>
				<variables>
					<variable>Obj</variable>
				</variables>
				<useForm/>
			</fillVariable>
			<fillVariable>
				<participant/>
				<condition/>
				<variables>
					<variable>Obj</variable>
				</variables>
				<useForm/>
				<subprogram>
					<xepCode>
						<condition/>
						<codetorun>Texto += "sub ";</codetorun>
					</xepCode>
				</subprogram>
			</fillVariable>
			<xepCode>
				<condition>Numero * 2 > 100 || Bool</condition>
				<codetorun>Texto += "Super";</codetorun>
			</xepCode>
