				<xs:element name="groups">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="group" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="attribute">
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="referenceToAttribute">
														<xs:attribute name="order">
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:enumeration value="asc"/>
																	<xs:enumeration value="desc"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
													</xs:extension>
												</xs:simpleContent>
